<?xml version="1.0" encoding="UTF-8" ?>
<xs:schema targetNamespace="https://xtuple.com/schema/php-xdruple.environment"
           xmlns="https://xtuple.com/schema/php-xdruple.environment"
           xmlns:application="https://xtuple.com/schema/php-xdruple.application"
           xmlns:xs="http://www.w3.org/2001/XMLSchema"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="https://xtuple.com/schema/php-xdruple.application application.xsd"
           elementFormDefault="qualified">
  <xs:complexType name="environment">
    <xs:choice>
    </xs:choice>
  </xs:complexType>

  <xs:element name="environment">
    <xs:complexType>
      <xs:complexContent>
        <xs:extension base="environment">
          <xs:attribute type="application:environment" name="type" use="required"/>
        </xs:extension>
      </xs:complexContent>
    </xs:complexType>
  </xs:element>
</xs:schema>
